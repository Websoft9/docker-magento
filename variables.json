{
  "name": "magento",
  "trademark": "Magento",
  "compose_file": "docker-compose.yml",
  "installpath": "/data/wwwroot/magento",
  "fork_url": "https://github.com/magento/magento-cloud-docker",
  "env_file": ".env",
  "requirements":{
    "cpu_arch": "Linux x86-64, ARM 32/64, Windows x86-64, IBM POWER8, x86/i686",
    "cpu": "2",
    "memory": "6",
    "disk": "20",
    "swap": "1",
    "url": "https://devdocs.magento.com/cloud/docker/docker-development.html#prerequisites"
  },

  "user": {
    "username": "admin",
    "password": "123456"
  },

  "services":[
    {
      "name": "magento-server",
      "port": "9002",
      "description_cn": "浏览器访问 Magento",
      "description_en": "Web-GUI database management tool",
      "required": "Y"
    },

    {
      "name": "phpmyadmin",
      "port": "9090",
      "description_cn": "数据库可视化管理工具",
      "description_en": "Web-GUI database management tool",
      "required": "Y"
    }
  ]

}